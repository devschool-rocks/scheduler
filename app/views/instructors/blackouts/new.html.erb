<div class="container">
  <div class="row">
    <div class="col-md-6 col-md-offset-3">
<%= form_for [:instructors, current_instructor.blackouts.build] do |f| %>
  <div class="form-group">
    <%= f.text_field :starts_at, autofocus: true, class: "form-control" %>
  </div>

  <div class="form-group">
    <%= f.text_field :ends_at,  class: "form-control" %>
  </div>

  <div class="form-group">
    <%= f.submit class: "btn btn-primary btn-block" %>
  </div>
          <%- end %>
    </div>
  </div>
</div>
